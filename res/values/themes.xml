<resources>

    <style name="OKTheme" parent="Theme.AppCompat">
        <item name="android:actionBarStyle">@style/UIActionBar</item>
        <item name="android:actionMenuTextAppearance">@style/UIActionBarTitle</item>
        <!--<item name="android:actionOverflowButtonStyle">@style/Widget.Styled.ActionButton.Overflow</item>-->
