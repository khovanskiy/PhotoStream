        <!-- Support library compatibility -->
        <item name="actionBarStyle">@style/UIActionBar</item>
        <item name="actionMenuTextAppearance">@style/UIActionBarTitle</item>
        <!--<item name="actionOverflowButtonStyle">@style/Widget.Styled.ActionButton.Overflow</item>-->
    </style>

    <style name="UIActionBar" parent="Widget.AppCompat.ActionBar.Solid">
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:background">@android:color/background_dark</item>
        <item name="android:icon">@android:color/transparent</item>
        <item name="android:titleTextStyle">@style/UIActionBarTitle</item>
        <!-- Support library compatibility -->
        <item name="windowActionBarOverlay">true</item>
        <item name="background">@android:color/background_dark</item>
        <item name="icon">@android:color/transparent</item>
        <item name="titleTextStyle">@style/UIActionBarTitle</item>
    </style>

    <style name="UIActionBarTitle" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">#FFF</item>
        <item name="android:textSize">20dp</item>
        <!-- The textColor property is backward compatible with the Support Library -->
    </style>

    <style name="Widget.Sherlock.ActionButton" parent="@android:style/Widget.Holo.ActionButton"/>

    <style name="Widget.Styled.ActionButton" parent="@style/Widget.Sherlock.ActionButton">
        <item name="android:paddingTop">8dip</item>
        <item name="android:paddingLeft">8dip</item>
        <item name="android:paddingRight">8dip</item>
        <item name="android:minWidth">48.0dip</item>
    </style>

    <style name="Widget.Styled.ActionButton.Overflow" parent="@style/Widget.Styled.ActionButton">
        <item name="android:paddingLeft">8dip</item>
        <item name="android:paddingRight">8dip</item>
        <item name="android:src">@drawable/actionbar_menu</item>
        <item name="android:minWidth">48.0dip</item>
        <item name="android:background">@drawable/standard_button_background</item>
    </style>

    <style name="LoginButton">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">@drawable/login_breen_btn_n</item>
        <item name="android:layout_margin">5.0dp</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">2.0</item>
    </style>

